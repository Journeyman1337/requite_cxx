[object Big
    [property x r:i32 2]
    [property y r:i32 1]
    [property z r:i32 3]
    [property w r:i32 0]
    [property a r:i32 0]
    [property b r:i32 0]
    [property c r:i32 0]
    [property d r:i32 0]
]

// the ir generated for this function stores the Big in a pointer argument instead of returning it.
[mangled_name test_func],
[function test_func Big

    [return Big{}]
]

[entry_point
    [local big Big test_func()]

    c:printf("%d\n" big.x)
]

