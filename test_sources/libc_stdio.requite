[module libc_stdio]

[export_group c

    [calling_convention C],
    [external_function puts r:i32 [arguments *r:ascii str]]

    [calling_convention C],
    [variadic_arguments],
    [external_function printf r:i32 [arguments *r:ascii format_str]]

]