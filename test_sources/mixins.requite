[object Mixin
    [constructor
        c:puts("mixin constructor")
    ]

    [destructor
        c:puts("mixin destructor")
    ]
]

[object Main
    [property mixin Mixin{}]

    [constructor
        c:puts("main constructor")
    ]

    [destructor
        c:puts("main destructor")
    ]
]

[entry_point
    [local main Main{}]
]