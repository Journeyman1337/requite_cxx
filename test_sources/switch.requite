[entry_point
    switch(1)
    switch(2)
    switch(0)
    switch(7)
    switch(10)
]

// requite has no keywords, so the function can be named switch and it works just fine!
[function switch r:void [arguments r:i32 int]

    [switch int
        [case 1
            c:puts("its 1")
        ]
        [case 2
            c:puts("its 2")
        ]
        [case 0
            [fall_through]
        ]
        [case 7
            c:puts("its 0 or 7")
        ]
        [default
            c:puts("its something else")
        ]    
    ]

]